<Window x:Class="PTwoManage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_File">
                <MenuItem Header="_Open"></MenuItem>
            </MenuItem>
            <MenuItem Header="_User">
                <MenuItem Header="_Add User" Click="AddUser_Click"></MenuItem>
                <MenuItem Header="_Edit User" Click="EditUser_Click"></MenuItem>
                <MenuItem Header="_Remove User" Click="RemoveUser_Click"></MenuItem>
            </MenuItem>
        </Menu>
        <StackPanel></StackPanel>
    </DockPanel>
    <!--<Grid/> Den var der til at starte med -->

</Window>
